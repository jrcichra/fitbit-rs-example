  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'fitbit-rs-gpt'",
      "cargo": {
        "args": ["build", "--bin=fitbit-rs-gpt", "--package=fitbit-rs-gpt"],
        "filter": {
          "name": "fitbit-rs-gpt",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'fitbit-rs-gpt'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=fitbit-rs-gpt",
          "--package=fitbit-rs-gpt"
        ],
        "filter": {
          "name": "fitbit-rs-gpt",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
